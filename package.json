{
  "name": "medx-contracts",
  "version": "0.1.0",
  "license": "MIT",
  "author": "PixelPlex inc.",
  "scripts": {
    "clean": "hardhat clean",
    "clean:deployments": "hardhat clean-deployments",
    "compile": "hardhat compile --force",
    "hh:node": "hardhat node",
    "test": "NODE_APP_INSTANCE=test hardhat test ./test/index.test.ts",
    "test:watch": "hardhat watch test",
    "test:node": "NODE_APP_INSTANCE=test hardhat test --network localhost ./test/index.test.ts",
    "coverage": "NODE_APP_INSTANCE=test hardhat coverage --testfiles ./test/index.test.ts",
    "gas-reporter": "ENABLE_GAS_REPORT=true hardhat test",
    "docgen": "NODE_APP_INSTANCE=test hardhat docgen",
    "lint": "yarn lint:ts && yarn lint:sol",
    "lint:fix": "yarn lint:ts:fix && yarn lint:sol:fix",
    "lint:ts": "eslint --ext .ts .",
    "lint:ts:fix": "eslint --ext .ts . --fix",
    "lint:sol": "solhint -f table '{contracts,test}/**/*.sol' && prettier --check './contracts/**/*.sol'",
    "lint:sol:fix": "prettier --write './contracts/**/*.sol'",
    "migrate:local": "NODE_APP_INSTANCE=local hardhat deploy --network localhost",
    "migrate:mainnet": "NODE_APP_INSTANCE=mainnet hardhat deploy --network mainnet",
    "migrate:sepolia": "NODE_APP_INSTANCE=sepolia hardhat deploy --network sepolia",
    "migrate:polygon": "NODE_APP_INSTANCE=polygon hardhat deploy --network polygon",
    "migrate:polygonMumbai": "NODE_APP_INSTANCE=polygonMumbai hardhat deploy --network polygonMumbai",
    "migrate:bsc": "NODE_APP_INSTANCE=bsc hardhat deploy --network bsc",
    "migrate:bscTestnet": "NODE_APP_INSTANCE=bscTestnet hardhat deploy --network bscTestnet",
    "verify:mainnet": "NODE_APP_INSTANCE=mainnet hardhat verify-by-name --network mainnet --contracts",
    "verify:sepolia": "NODE_APP_INSTANCE=sepolia hardhat verify-by-name --network sepolia --contracts",
    "verify:polygon": "NODE_APP_INSTANCE=polygon hardhat verify-by-name --network polygon --contracts",
    "verify:polygonMumbai": "NODE_APP_INSTANCE=polygonMumbai hardhat verify-by-name --network polygonMumbai --contracts",
    "verify:bsc": "NODE_APP_INSTANCE=bsc hardhat verify-by-name --network bsc --contracts",
    "verify:bscTestnet": "NODE_APP_INSTANCE=bscTestnet hardhat verify-by-name --network bscTestnet --contracts"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.7",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "chai": "^4.3.10",
    "config": "^3.3.8",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.9.0",
    "fs-extra": "^11.2.0",
    "hardhat": "^2.22.2",
    "hardhat-deploy": "^0.12.1",
    "hardhat-docgen": "^1.3.0",
    "hardhat-exposed": "^0.3.15",
    "hardhat-gas-reporter": "^2.2.0",
    "hardhat-watcher": "^2.5.0",
    "mocha": "^10.2.0",
    "prettier": "^3.3.2",
    "prettier-plugin-solidity": "^1.3.1",
    "solhint": "^5.0.1",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.2",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.1.1",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  }
}
